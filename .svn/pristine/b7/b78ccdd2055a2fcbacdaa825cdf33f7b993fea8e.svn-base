	a.DutyDeptName,a.DeptName 
	FROM cs_pub_dtinfo a
	WHERE 1=1 
	<!-- 认领接收查询标志 -->
	<if test="claimFlag !=null and claimFlag == '1'.toString()">
	   and CheckDep is 
       not null and CheckPushType != '0' and CheckPushType !='3' AND ReceiveState != '1' AND ClaimState !='1'  AND checkStopState !='1'  
