	 SELECT   
	    S.UID,
		S.PriPID,
		S.UniSCID,
		S.EntName,
		S.RegNO,
		S.LeRep,
		S.Dom,
		S.ApprDate,
		S.ExaName,
		S.CheckRegType,
		S.CheckPushType,
		S.CheckPushDate,
		S.CheckDeptType,
		S.AcceptUser,
		S.AcceptName,
		S.AcceptDate,
		S.ReassignAccount,
		S.ReassignName,
		S.ReassignDate,
		S.ReassignType,
		S.ReassignRemark,
		S.ReassignDeptName,
		S.ReassignCode,
		S.ClaimCode,
		S.ClaimName,
		S.ClaimDate,
		S.ReceiveState,
		S.ClaimState,
		S.OpScope, 
		S.DutyDeptName,
		S.DeptName,
		S.CheckDepName,
		S.RegOrg,
		S.LocalAdm, 
		b.Content regOrgName,
		c.Content localAdmName 
	 FROM (
	  SELECT   
	    a.UID,
		a.PriPID,
		a.UniSCID,
		a.EntName,
		a.RegNO,
		a.LeRep,
		a.Dom,
		a.ApprDate,
		a.ExaName,
		a.CheckRegType,
		a.CheckPushType,
		a.CheckPushDate,
		a.CheckDeptType,
		a.AcceptUser,
		a.AcceptName,
		a.AcceptDate,
		a.ReassignAccount,
		a.ReassignName,
		a.ReassignDate,
		a.ReassignType,
		a.ReassignRemark,
		a.ReassignDeptName,
		a.ReassignCode,
		a.ClaimCode,
		a.ClaimName,
		a.ClaimDate,
		a.ReceiveState,
		a.ClaimState,
		a.OpScope, 
		a.DutyDeptName,
		a.DeptName,
		a.CheckDepName,
		a.RegOrg,
		a.LocalAdm
	  FROM cs_pub_dtinfo  a WHERE 
	 <![CDATA[
		 a.checkStopState!='1'
	 ]]>
	 <include refid="pubdtinfo_where"></include>
	 <!-- 数据权限 -->
 	 <include refid="com.icinfo.cs.system.mapper.CSMapper.defaultByOrgCode"/>
	 LIMIT ${numStart},${pageSize}
	 )S  
	 LEFT JOIN  cs_code_regorg b  ON  S.RegOrg=b.code
	 LEFT JOIN cs_code_regunit c ON  S.LocalAdm=c.code  
	 order by S.CheckPushDate desc
  </select>
  
  
   <!-- 查询部门未认领的审批事项 -->
  <select id="selectTransferPubdtinfoList" resultMap="PubdtinfoFailReBackResultMap" parameterType="Map">
	    SELECT
			c.UID,
			c.PriPID,
			c.UniSCID,
			c.EntName,
			c.RegNO,
			c.LeRep,
			c.Dom,
			c.ApprDate,
			c.ExaName,
			c.CheckRegType,
			c.CheckPushType,
			c.CheckPushDate,
			c.CheckDeptType,
			c.AcceptUser,
			c.AcceptName,
			c.AcceptDate,
			b.DomDistrict
		FROM
			(
				SELECT
					a.UID,
					a.PriPID,
					a.UniSCID,
					a.EntName,
					a.RegNO,
					a.LeRep,
					a.Dom,
					a.ApprDate,
					a.ExaName,
					a.CheckRegType,
					a.CheckPushType,
					a.CheckPushDate,
					a.CheckDeptType,
					a.AcceptUser,
					a.AcceptName,
					a.AcceptDate
				FROM
					cs_pub_dtinfo a
				WHERE
					a.AcceptDate IS NULL
				AND a.ClaimDate IS NULL
				AND a.checkStopState != '1'
				AND a.ReassignType is null 
				AND a.CheckPushType in ('1')
				AND a.CheckDep = #{checkDep}
				and a.exaCode   in (${exaCode}) 
			) c
		LEFT JOIN cs_mid_baseinfo b ON c.PriPID = b.PriPID
		WHERE
			LEFT (b.Yiedistrict, 6) in (${areaCode}) 
  </select>
  
