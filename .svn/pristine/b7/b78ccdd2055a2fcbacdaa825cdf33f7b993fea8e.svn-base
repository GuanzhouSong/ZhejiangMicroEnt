   <select id="selectClaimInfoForDuty" resultMap="PubdtinfoFailReBackResultMap" parameterType="Map">
   		SELECT
		t.DutyDeptName,
		t.DutyDeptCode,
		t.pushCount,
		t.claimCount,
    	ROUND(IFNULL(
			(t.claimCount / t.pushCount) * 100,
			0
		),2) claimRate
		FROM
		(
			SELECT
				DutyDeptName,
				DutyDeptCode,
				COUNT(id) pushCount,
        		SUM(CASE WHEN ClaimState ='1' THEN 1 ELSE 0 END ) claimCount
			FROM
				cs_pub_dtinfo
			WHERE
				CheckPushType IN ('1', '2')
			AND checkStopState = '0'
			AND CheckDeptType = '1'
			<include refid="regcharts_where"></include>
			GROUP BY
				DutyDeptCode
		) t
	ORDER BY
		t.pushCount DESC
		limit 10
   </select>
   
    <select id="selectClaimInfoForArea" resultMap="PubdtinfoFailReBackResultMap" parameterType="Map">
          SELECT
			temp.orgCode area,
			IFNULL(a.pushCount, 0) pushCount,
			IFNULL(a.claimCount, 0) claimCount,
			IFNULL(SUM(b.backCount), 0) backCount,
			ROUND(
				IFNULL(
					(a.claimCount / a.pushCount) * 100,
					0
				),
				2
			) claimRate
		FROM
			(
				SELECT
					LEFT (t. CODE, 4) orgCode
				FROM
					cs_code_regorg t
				WHERE
					CODE != '100000'
				AND CODE != '999999'
				GROUP BY
					LEFT (t. CODE, 4)
			) temp
		LEFT JOIN (
			SELECT
				LEFT (CheckDep, '4') areaCode,
				COUNT(id) pushCount,
				SUM(
					CASE
					WHEN ClaimState = '1' THEN
						1
					ELSE
						0
					END
				) claimCount
			FROM
				cs_pub_dtinfo
			WHERE
				CheckPushType IN ('1', '2')
			AND checkStopState = '0'
			AND CheckDeptType = '1'
			<include refid="regcharts_where"></include>
			GROUP BY
				LEFT (CheckDep, '4')
		) a ON temp.orgCode = a.areaCode
		LEFT JOIN (
			SELECT
				LEFT (CheckDep, '4') areaCode,
				COUNT(0) backCount
			FROM
				cs_pub_dtinfo
			WHERE
				CheckPushType IN ('0')
			AND checkStopState = '0'
			AND CheckDeptType = '1'
			<include refid="regcharts_where"></include>
			GROUP BY
				LEFT (CheckDep, '4')
		) b ON temp.orgCode = b.areaCode
	GROUP BY
		temp.orgCode
    </select>
   
