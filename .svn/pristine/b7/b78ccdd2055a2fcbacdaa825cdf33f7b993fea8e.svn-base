	<!-- 涉证统计 -->
	<select id="selectPubDtInfoCountRlt" resultMap="PubdtinfoCountRlt" parameterType="Map">
 	      SELECT
				a.checkarea,
				a.deptcode,
				IFNULL(sum(a.pushDoorCount), 0) pushDoorCount,
				IFNULL(sum(a.pushCount), 0) pushCount,
				IFNULL(sum(a.noClaimDoorCount), 0) noClaimDoorCount,
				IFNULL(sum(a.noClaimCount), 0) noClaimCount,
				IFNULL(sum(a.BackDoorCount), 0) backDoorCount,
				IFNULL(sum(a.BackCount), 0) backCount,
				IFNULL(
					sum(a.checkClaimDoorCount),
					0
				) ClaimDoorCount,
				IFNULL(sum(a.checkClaimCount), 0) ClaimCount,
				IFNULL(
					sum(a.checkClaimCount) + sum(a.backCount),
					0
				) checkHandleClaimCount,
				round(
					IFNULL(
						 ((sum(a.checkClaimCount) + sum(a.backCount)) /sum(a.pushCount)) * 100,
						0
					),
					2
				) claimRate,
				a.DeptName,
				a.areaName areaName
			FROM
				(
				SELECT
						substr(CheckDep, 1, 6) checkarea,
						CheckDep AS deptcode,
						count(DISTINCT PriPID) pushDoorCount,
						0 pushCount,
						0 noClaimDoorCount,
						0 noClaimCount,
						0 BackDoorCount,
						0 BackCount,
						0 checkClaimDoorCount,
						0 checkClaimCount,
						CheckDepName DeptName,
						AreaName areaName
					FROM
						cs_pub_dtinfo
					WHERE
					<![CDATA[
						CheckPushType IN ('1', '2', '0')
					AND checkStopState != '1'
					AND CheckDeptType = '1'
					]]>
					<include refid="pubdtinfoCount_where"></include>
					GROUP BY
						PriPID,CheckDep
					UNION ALL
					SELECT
						substr(CheckDep, 1, 6) checkarea,
						CheckDep AS deptcode,
						0 pushDoorCount,
						count(PriPID) pushCount,
						0 noClaimDoorCount,
						0 noClaimCount,
						0 BackDoorCount,
						0 BackCount,
						0 checkClaimDoorCount,
						0 checkClaimCount,
						CheckDepName DeptName,
						AreaName areaName
					FROM
						cs_pub_dtinfo
					WHERE
					<![CDATA[
						CheckPushType IN ('1', '2', '0')
					AND checkStopState != '1'
					AND CheckDeptType = '1'
					]]>
					<include refid="pubdtinfoCount_where"></include>
					GROUP BY
						PriPID,CheckDep
					UNION ALL
						SELECT
							substr(CheckDep, 1, 6) checkarea,
							CheckDep AS deptcode,
							0 pushDoorCount,
							0 pushCount,
							count(DISTINCT PriPID) noClaimDoorCount,
							count(PriPID) noClaimCount,
							0 BackDoorCount,
							0 BackCount,
							0 checkClaimDoorCount,
							0 checkClaimCount,
							CheckDepName DeptName,
							AreaName areaName
						FROM
							cs_pub_dtinfo
						WHERE
						<![CDATA[
							CheckPushType IN ('1', '2')
						AND checkStopState != '1'
						AND CheckDeptType = '1'
						AND (
							ClaimDate IS NULL
							OR ClaimDate = ''
						)
						AND ClaimState != '1'
						]]>
						<include refid="pubdtinfoCount_where"></include>
						GROUP BY
							PriPID,CheckDep
						UNION ALL
							SELECT
								substr(CheckDep, 1, 6) checkarea,
								CheckDep AS deptcode,
								0 pushDoorCount,
								0 pushCount,
								0 noClaimDoorCount,
								0 noClaimCount,
								count(DISTINCT PriPID) BackDoorCount,
								count(PriPID) BackCount,
								0 checkClaimDoorCount,
								0 checkClaimCount,
								CheckDepName DeptName,
								AreaName areaName
							FROM
								cs_pub_dtinfo
							WHERE
							<![CDATA[
								CheckPushType = '0'
							AND checkStopState != '1'
							AND CheckDeptType = '1'
							AND ClaimState != '1'
							]]>
							<include refid="pubdtinfoCount_where"></include>
							GROUP BY
								PriPID,CheckDep
							UNION ALL
								SELECT
									substr(CheckDep, 1, 6) checkarea,
									CheckDep AS deptcode,
									0 pushDoorCount,
									0 pushCount,
									0 noClaimDoorCount,
									0 noClaimCount,
									0 BackDoorCount,
									0 BackCount,
									count(DISTINCT PriPID) checkClaimDoorCount,
									count(PriPID) checkClaimCount,
									CheckDepName DeptName,
									AreaName areaName
								FROM
									cs_pub_dtinfo
								WHERE
								<![CDATA[
									CheckPushType IN ('1', '2')
								AND checkStopState != '1'
								AND CheckDeptType = '1'
								AND ClaimState = '1'
								AND ClaimDate IS NOT NULL
								AND ClaimDate != ''
								]]>
								<include refid="pubdtinfoCount_where"></include>
								GROUP BY
									PriPID,CheckDep
				) a
			GROUP BY
				deptcode
	</select>
	
  <!-- 涉证企业推送与反馈统计   暂时不用-->
  <select id="selectPubDtInfoCount" resultMap="inputCountRlt" parameterType="Map">
			SELECT
				d.DeptName,
				d.pushDoorCount,
				d.pushCount,
				d.checkClaimDoorCount ClaimDoorCount,
				d.checkClaimCount ClaimCount,
				d.toReportDoorCount,
				d.toReportReceiveDoorCount,
				d.toReportReceiveCount,
				d.checkCount,
				d.peerCount,
				round(d.claimRate, 2) claimRate,
				round(d.receiveRate, 2) receiveRate,
				d.areaName
			FROM
				(
					SELECT
						b.checkarea,
						b.DeptName,
						b.pushDoorCount,
						b.pushCount,
						b.checkClaimDoorCount,
						b.checkClaimCount,
						b.toReportDoorCount,
						b.toReportReceiveDoorCount,
						b.toReportReceiveCount,
						b.checkCount,
						b.peerCount,
						b.claimRate,
						b.receiveRate,
						b.areaName
					FROM
						(
							SELECT
								a.checkarea,
								a.deptcode,
								IFNULL(sum(a.pushDoorCount), 0) pushDoorCount,
								IFNULL(sum(a.pushCount), 0) pushCount,
								IFNULL(
									sum(a.checkClaimDoorCount),
									0
								) checkClaimDoorCount,
								IFNULL(sum(a.checkClaimCount), 0) checkClaimCount,
								IFNULL(sum(a.toReportDoorCount), 0) toReportDoorCount,
								IFNULL(
									sum(a.toReportReceiveDoorCount),
									0
								) toReportReceiveDoorCount,
								IFNULL(
									sum(a.toReportReceiveCount),
									0
								) toReportReceiveCount,
								IFNULL(sum(a.checkCount), 0) checkCount,
								IFNULL(sum(a.peerCount), 0) peerCount,
								IFNULL(
									sum(a.checkClaimCount) / sum(a.checkCount) * 100,
									0
								) claimRate,
								IFNULL(
									sum(a.toReportReceiveCount) / sum(a.peerCount) * 100,
									0
								) receiveRate,
			          			a.DeptName,
								a.areaName
							FROM
								(
									SELECT
										substr(CheckDep, 1, 6) checkarea,
										CheckDep AS deptcode,
										count(DISTINCT PriPID) pushDoorCount,
										count(PriPID) pushCount,
										0 checkClaimDoorCount,
										0 checkClaimCount,
										0 checkCount,
										0 toReportDoorCount,
										0 peerCount,
										0 toReportReceiveDoorCount,
										0 toReportReceiveCount,
										CheckDepName DeptName,
			             				AreaName areaName
									FROM
										cs_pub_dtinfo
									WHERE
									<![CDATA[
									LENGTH(CheckDep)=13
									AND CheckPushType IN ('1', '2')
									AND checkStopState != '1'
									]]>
									<include refid="pubdtinfoCount_where"></include>
									GROUP BY
									    Area,
										CheckDep
									UNION ALL
										SELECT
											substr(CheckDep, 1, 6) checkarea,
											CheckDep AS deptcode,
											0 pushDoorCount,
											0 pushCount,
											count(DISTINCT PriPID) checkClaimDoorCount,
											count(PriPID) checkClaimCount,
											0 checkCount,
											0 toReportDoorCount,
											0 peerCount,
											0 toReportReceiveDoorCount,
											0 toReportReceiveCount,
											CheckDepName DeptName,
											AreaName areaName
										FROM
											cs_pub_dtinfo
										WHERE
										<![CDATA[
										LENGTH(CheckDep)=13
										AND CheckDeptType = '1'
										AND ClaimState = '1'
										AND checkStopState != '1'
										]]>
										<include refid="pubdtinfoCount_where"></include>
										GROUP BY
											Area,
											CheckDep
										UNION ALL
											SELECT
												substr(CheckDep, 1, 6) checkarea,
												CheckDep AS deptcode,
												0 pushDoorCount,
												0 pushCount,
												0 checkClaimDoorCount,
												0 checkClaimCount,
												count(PriPID) checkCount,
												0 toReportDoorCount,
												0 peerCount,
												0 toReportReceiveDoorCount,
												0 toReportReceiveCount,
												CheckDepName DeptName,
												AreaName areaName
											FROM
												cs_pub_dtinfo
											WHERE
											<![CDATA[
											LENGTH(CheckDep)=13
											AND CheckDeptType = '1'
											AND CheckPushType IN ('1', '2')
											AND checkStopState != '1'
											]]>
											<include refid="pubdtinfoCount_where"></include>
											GROUP BY
												Area,
												CheckDep
											UNION ALL
												SELECT
													substr(CheckDep, 1, 6) checkarea,
													CheckDep AS deptcode,
													0 pushDoorCount,
													0 pushCount,
													0 checkClaimDoorCount,
													0 checkClaimCount,
													0 checkCount,
													count(DISTINCT PriPID) toReportDoorCount,
													count(PriPID) peerCount,
													0 toReportReceiveDoorCount,
													0 toReportReceiveCount,
													CheckDepName DeptName,
													AreaName areaName
												FROM
													cs_pub_dtinfo
												WHERE
												<![CDATA[
													CheckDeptType != '1'
												AND CheckPushType IN ('1', '2')
												AND checkStopState != '1'
												]]>
												<include refid="pubdtinfoCount_where"></include>
												GROUP BY
													Area,
													CheckDep
												UNION ALL
													SELECT
														substr(CheckDep, 1, 6) checkarea,
														CheckDep AS deptcode,
														0 pushDoorCount,
														0 pushCount,
														0 checkClaimDoorCount,
														0 checkCount,
														0 checkClaimCount,
														0 toReportDoorCount,
														0 peerCount,
														count(DISTINCT PriPID) toReportReceiveDoorCount,
														count(PriPID) toReportReceiveCount,
														CheckDepName DeptName,
														AreaName areaName
													FROM
														cs_pub_dtinfo
													WHERE
													<![CDATA[
														ReceiveState = '1'
													AND CheckDeptType != '1'
													AND checkStopState != '1'
													AND CheckPushType IN ('1', '2')
													]]>
													<include refid="pubdtinfoCount_where"></include>
													GROUP BY
														Area,
														CheckDep
								) a
							GROUP BY
								checkarea,
								deptcode
						) b
			 	) d
  </select>
  
  <!-- 查询待手动分流的企业（分流失败和退回待推送） -->
