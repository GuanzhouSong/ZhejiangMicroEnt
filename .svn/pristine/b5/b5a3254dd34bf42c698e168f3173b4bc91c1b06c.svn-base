	<select id="selectScPtypeTreeMap" resultMap="treeMap">
     select t.Code id,t.Content NAME,t.PCode pId FROM cs_pub_sccheck_type t
  </select>
	<select id="selectScPtypeList" resultMap="PubScagentDto">
     select t.Code code,t.Content content FROM cs_pub_sccheck_type t
  </select>
  
   <sql id="scAgentcount_where">
  		<if test="statArea !='' and statArea !=null">
	    and  LEFT(t.SlicenNO, 4)  IN  (${statArea}) 
	    </if>
  		<if test="entTypes != null and entTypes != ''">
			and t.EntType in (${entTypes}) 
		</if>
		<if test="setTimeStart !='' and setTimeStart !=null">
	      <![CDATA[
		  and DATE_FORMAT(t.SetTime,'%Y-%m-%d') >= #{setTimeStart}
		  ]]>
	     </if>
	     <if test="setTimeEnd !='' and setTimeEnd !=null">
	      <![CDATA[
		  and DATE_FORMAT(t.SetTime,'%Y-%m-%d') <= #{setTimeEnd}
		  ]]>
	     </if>
		 <if test="startCheckPushDate !='' and startCheckPushDate !=null">
			<![CDATA[
			and DATE_FORMAT(t.setTime,'%Y%m') >= left(#{startCheckPushDate},6) 
			]]> 
		 </if>
		<if test="endCheckPushDate !='' and endCheckPushDate !=null">
		<![CDATA[
		   and DATE_FORMAT(t.setTime,'%Y%m') <= left(#{endCheckPushDate},6) 
		]]> 
		</if>
		 <if test="dutyDeptCodeArr !=null and dutyDeptCodeArr!=''">
			<foreach item="item" index="index" collection="dutyDeptCodeArr" open="AND ("
				separator="OR" close=")">
				t.DutyDeptCode like concat('%',#{item},'%')
			</foreach>
		</if>
  </sql>
  
  
   <select id="selectScAgentCount" resultMap="PubScagentDto" parameterType="Map">
	<if test="statLevel != null and statLevel != '' and  statLevel != 'area'">
	SELECT D.areaCode,D.SlicenNO,
	D.DeptCatgCount,D.DeptCatgGs,D.DeptCatgSp,D.DeptCatgZl,D.DeptCatgAq,D.DeptCatgWj,
	D.ExpertCount,D.ExpertBh,D.ExpertYpGSP,D.ExpertYpGMP,D.ExpertYlGSP,D.ExpertYlGMP,
	b.content deptName
	FROM
	(
	</if>
	SELECT LEFT(t.SlicenNO, 4) areaCode,
	<if test="statLevel != null and statLevel != '' and  statLevel == 'area' ">
	LEFT(t.SlicenNO, 4) deptName,
	</if>
	<if test="statLevel != null and statLevel != '' and  statLevel == 'regOrg' ">
	LEFT(t.SlicenNO, 6) SlicenNO,
	</if>
	<if test="statLevel != null and statLevel != '' and  statLevel == 'localAdm' ">
	t.SlicenNO SlicenNO,
	</if>
	SUM(1) DeptCatgCount,
	SUM(CASE WHEN locate(',A058-001,',concat(',',t.DeptCatg,',')) >0 THEN 1 ELSE 0 END) DeptCatgGs,
	SUM(CASE WHEN locate(',A058-002,',concat(',',t.DeptCatg,',')) >0 THEN 1 ELSE 0 END) DeptCatgSp,
	SUM(CASE WHEN locate(',A058-003,',concat(',',t.DeptCatg,',')) >0 THEN 1 ELSE 0 END) DeptCatgZl,
	SUM(CASE WHEN locate(',A058-004,',concat(',',t.DeptCatg,',')) >0 THEN 1 ELSE 0 END) DeptCatgAq,
	SUM(CASE WHEN locate(',A058-005,',concat(',',t.DeptCatg,',')) >0 THEN 1 ELSE 0 END) DeptCatgWj,
	SUM(CASE WHEN t.ExpertFlag !='N' THEN 1 ELSE 0 END) ExpertCount,
	SUM(CASE WHEN  locate(',A058-001,',CONCAT(t.ExpertFlag,','))>0 THEN 1 ELSE 0 END) ExpertBh,
	SUM(CASE WHEN  locate(',A058-002,',CONCAT(t.ExpertFlag,','))>0 THEN 1 ELSE 0 END) ExpertYpGSP,
	SUM(CASE WHEN  locate(',A058-003,',CONCAT(t.ExpertFlag, ',')) > 0 THEN 1 ELSE 0 END) ExpertYpGMP,
	SUM(CASE WHEN  locate(',A058-004,',CONCAT(t.ExpertFlag,','))>0 THEN 1 ELSE 0 END) ExpertYlGSP,
	SUM(CASE WHEN  locate(',A058-005,',CONCAT(t.ExpertFlag,','))>0 THEN 1 ELSE 0 END) ExpertYlGMP
	FROM cs_pub_scagent t 
	WHERE t.SlicenNO IS NOT NULL and t.SlicenNO != ''
	and t.AgentState = '1'
	<include refid="scAgentcount_where"/>
	GROUP BY LEFT(t.SlicenNO, 4)
	<if test="statLevel != null and statLevel != '' and  statLevel == 'regOrg' ">
	,LEFT(t.SlicenNO, 6)
	) D
	LEFT JOIN 
	cs_code_regorg b
	on b.code = D.SlicenNO
	</if>
	<if test="statLevel != null and statLevel != '' and  statLevel == 'localAdm' ">
    ,t.SlicenNO
    ) D
	LEFT JOIN 
	cs_code_regunit b
	on b.code = D.SlicenNO
	</if>
  </select>
  
  <select id="selectSynScAgentCount" resultMap="PubScagentDto" parameterType="Map">
	SELECT 
		LEFT(t.SlicenNO,4) areaCode,t.SlicenNOName,
		SUM(1) DeptCatgCount,
		SUM(CASE WHEN LOCATE('A014-001',t.DeptCatg) > 0 THEN 1 ELSE 0 END) DeptCatg1,
		SUM(CASE WHEN LOCATE('A014-002',t.DeptCatg) > 0 THEN 1 ELSE 0 END) DeptCatg2,
		SUM(CASE WHEN LOCATE('A014-003',t.DeptCatg) > 0 THEN 1 ELSE 0 END) DeptCatg3,
		SUM(CASE WHEN LOCATE('A014-005',t.DeptCatg) > 0 THEN 1 ELSE 0 END) DeptCatg5,
		SUM(CASE WHEN LOCATE('A014-006',t.DeptCatg) > 0 THEN 1 ELSE 0 END) DeptCatg6,
		SUM(CASE WHEN LOCATE('A014-007',t.DeptCatg) > 0 THEN 1 ELSE 0 END) DeptCatg7,
		SUM(CASE WHEN t.ExpertFlag !='N' THEN 1 ELSE 0 END) ExpertCount
	FROM cs_pub_scagent t
	WHERE t.AgentState = '1'
	<include refid="scAgentcount_where"/>
	<if test="statLevel != null and statLevel != '' and  statLevel == 'area' ">
		GROUP BY LEFT(t.SlicenNO,4)
	</if>
	<if test="statLevel != null and statLevel != '' and  statLevel == 'check' ">
		GROUP BY t.SlicenNO
	</if>
  </select>
</mapper>