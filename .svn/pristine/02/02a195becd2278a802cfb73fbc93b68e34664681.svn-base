*{ margin:0; padding:0;}
body{ margin:0 auto; font-size:14px;}
ul,li{ padding:0; margin:0; list-style:none;}

.button{ width:400px; margin:0 auto; text-align:center;margin-bottom:5px; padding-top:10px; padding-bottom:5px}
.button a{ background-color:#E4E4E4; border:1px solid #666; padding:3px 10px; _padding:5px 10px; margin-left:10px; color:#666;}
.button a:hover{ background-color:#CCCCCC; color:#000;}

/* .print{ width:630px; margin:0 auto; line-height:28px;} */
.print{ width:630px; margin:0 auto; line-height:28px;page-break-after:auto; height:1030px; *height:1047px; _height:1045px; position: relative }
.print h1{ font-size:28px; text-align:center; margin:15px 0; font-weight:bold;}
.print h2{ font-size:20px; text-align:center; margin:5px 0; font-weight:bold;}
.print h3{ font-size:18px;margin:5px 0; font-weight:normal;}
.print table{border-collapse:collapse; width:100%; line-height:22px; font-size:14px;}
.print table td{ border:1px solid #000; padding:5px 1px; text-align:center;}
.print table th{ border:1px solid #000; padding:5px; text-align:left; font-weight:normal;}

