    <!-- 当天新设数量 -->
    <select id="selectDayXsCount" resultType="Integer" parameterType="Map">
    select count(1) xsCount 
    from  hz_qyhznr t 
    where <![CDATA[ESTDATE = #{nowDate} 
    and update_time > #{nowDate}]]>
    and REGORG is not null
    and OPSTATE in ('A','B','K','Q','X')
    <if test="type!=null and type!=''and type=='qy'">
       and qylxdl NOT IN ('50','16','17')
    </if>
    <if test="type!=null and type!=''and type=='nz'">
       and qylxdl in ('16','17')
    </if>
    <if test="type!=null and type!=''and type=='gt'">
       and qylxdl ='50'
    </if>
    <if test="areaCode !=null and areaCode !=''">
       and SUBSTR(REGORG FROM 1 FOR 4) = #{areaCode}
    </if>
    </select>
    
    <!-- 当天注销数量 -->
    <select id="selectDayZxCount" resultType="Integer" parameterType="Map">
    select count(1) xwCount 
    from bf_qyhznr 
    where <![CDATA[OPSTATE in('X','D','DA')  and HZRQ = #{nowDate}
    and update_time > #{nowDate}]]>
    and REGORG is not null
    <if test="type!=null and type!=''and type=='qy'">
       and qylxdl NOT IN ('50','16','17')
    </if>
    <if test="type!=null and type!=''and type=='nz'">
       and qylxdl in ('16','17')
    </if>
    <if test="type!=null and type!=''and type=='gt'">
       and qylxdl ='50'
    </if>
    <if test="areaCode !=null and areaCode !=''">
       and SUBSTR(REGORG FROM 1 FOR 4) = #{areaCode}
    </if>
    </select>
    
    <!-- 查询当前年度主体总数及同比增长率 -->
    <select id="selectZtCount" resultMap="BaseDtoResultMap" parameterType="Map">
    SELECT A.ZtTotal,B.preZtTotal+C.preZtTotal preZtTotal,
    ROUND((A.ZtTotal-(B.preZtTotal+C.preZtTotal))*100/((B.preZtTotal+C.preZtTotal)),2) YearAddRoate FROM 
	(SELECT COUNT(1) ZtTotal FROM hz_qyhznr WHERE QYLXDL IS NOT NULL and OPSTATE in ('A','B','K','Q','X') )A,
	(SELECT COUNT(1) preZtTotal FROM hz_qyhznr WHERE <![CDATA[ESTDATE < CONCAT(#{nowyear},'-01-01')]]> and OPSTATE in ('A','B','K','Q','X'))B,
	(SELECT COUNT(1) preZtTotal FROM bf_qyhznr WHERE OPSTATE IN ('X','D','DA')      
	<![CDATA[AND ESTDATE < CONCAT(#{nowyear},'-01-01') AND HZRQ >= CONCAT(#{nowyear},'-01-01')]]>)C 
    </select>
    
    <!-- 查询当前年度各分类主体总数 -->
    <select id="selectTypeZtCount" resultMap="BaseDtoResultMap" parameterType="Map">
	SELECT TYPE,SUM(num) TypeCount FROM (
	SELECT QYLXDL,
	CASE WHEN qylxdl NOT IN ('50','16','17') THEN 'qy' 
	     WHEN qylxdl IN ('16','17') THEN  'nz'
	     WHEN qylxdl = '50' THEN 'gt' END TYPE ,
	COUNT(1) num FROM hz_qyhznr WHERE QYLXDL IS NOT NULL and OPSTATE in ('A','B','K','Q','X')
	 GROUP BY QYLXDL
	) A  GROUP BY TYPE;
    </select>
    
    <!-- 当前年份最新月新设消亡数量 -->
    <select id="selectXsXwCount" resultMap="BaseDtoResultMap" parameterType="Map">
	<![CDATA[ 
	SELECT A.mouthcode,A.num xsCount,B.num xwCount FROM
	(SELECT #{nowMouthcode} mouthcode,
	COUNT(1) num FROM hz_qyhznr WHERE QYLXDL IS NOT NULL and OPSTATE in ('A','B','K','Q','X') 
	 AND ESTDATE >= #{estDateBegin} AND ESTDATE < SYSDATE()
	) A LEFT JOIN 
	(SELECT #{nowMouthcode} mouthcode,COUNT(1) num FROM bf_qyhznr 
	WHERE opstate IN ('X','D','DA') AND QYLXDL IS NOT NULL 
	AND HZRQ >= #{estDateBegin} AND HZRQ < SYSDATE())B
    ON A.mouthcode = B.mouthcode]]>
    </select>
    
    <!-- 查询股东分类(按照股东类型) -->
    <select id="selectinvTypeCount" resultMap="BaseDtoResultMap" parameterType="Map">
    SELECT TZFLX invType,COUNT(1) invTypeCount FROM hz_gdczmx  t  WHERE t.TZFLX IN (1,2,3)
    <if test="groupFlag != null and groupFlag != ''">
    GROUP BY TZFLX
    </if>
    </select>
    
    <!-- 查询股东区域分类 -->
    <select id="selectAreainvTypeCount" resultMap="BaseDtoResultMap" parameterType="Map">
	SELECT SUBSTR(b.regorg FROM 1 FOR 4) areacode,TZFLX invType, COUNT(1) invTypeCount 
	FROM hz_gdczmx a  LEFT JOIN hz_qyhznr b ON a.PRIPID = b.PRIPID WHERE b.REGORG IS NOT NULL and b.OPSTATE in ('A','B','K','Q','X')
	and a.TZFLX IN (1,2,3) 
	<if test="areaCode != null and areaCode != ''">
	AND SUBSTR(b.regorg FROM 1 FOR 4) = #{areaCode}
	</if>
	GROUP BY TZFLX
    </select>
    
    <!-- 查询股东行业占比 -->
    <select id="selectIndustryinvTypeCount" resultMap="BaseDtoResultMap" parameterType="Map">
     SELECT M.INDUSTRYCO hydm,M.indNum,N.Content hymc FROM 
	 (SELECT INDUSTRYCO,COUNT(1) indNum FROM hz_gdczmx a  LEFT JOIN hz_qyhznr b ON a.PRIPID = b.PRIPID 
	  WHERE b.REGORG IS NOT NULL and b.OPSTATE in ('A','B','K','Q','X')
	  <!-- 法人 -->
	  <if test="invType != null and invType != '' and invType =='1'.toString()">
	   and a.TZFLX IN(1,3)
	  </if>
	  <if test="invType != null and invType != '' and invType =='2'.toString()">
	   and a.TZFLX = 2
	  </if>
	  AND b.INDUSTRYCO IS NOT NULL AND b.INDUSTRYCO != ''
	   GROUP BY b.INDUSTRYCO ORDER BY indNum DESC LIMIT 5) M
	   LEFT JOIN 
	   bm_hyml N
	   ON M.INDUSTRYCO = N.CODE
    </select>
    
    <!-- 股东行业总数 -->
    <select id="selectIndustryinvTypeTotalCount" resultType="Integer" parameterType="Map">
     SELECT COUNT(1) indNum FROM hz_gdczmx a  LEFT JOIN hz_qyhznr b ON a.PRIPID = b.PRIPID 
	  WHERE b.REGORG IS NOT NULL and b.OPSTATE in ('A','B','K','Q','X')
	  <!-- 法人 -->
	  <if test="invType != null and invType !='' and invType=='1'.toString()">
	   and a.TZFLX IN(1,3)
	  </if>
	  <if test="invType != null and invType !='' and invType =='2'.toString()">
	   and a.TZFLX = 2
	  </if>
	  AND b.INDUSTRYCO IS NOT NULL AND b.INDUSTRYCO != ''
    </select>
    
    <!-- 5分钟内新设增加数据 -->
    <select id="selectHzMapList" resultMap="BaseDtoResultMap" parameterType="Map">
    SELECT  REGORG,COUNT(0) xsNum FROM  hz_qyhznr t WHERE t.OPSTATE IN ('K','A','B','Q','X') 
    AND REGORG is not null AND ESTDATE = #{nowDate} 
    AND update_time > DATE_SUB(NOW(),interval 5 MINUTE) GROUP BY REGORG
    </select>
    
    <!-- 5分钟内注销增加数据 -->
    <select id="selectBfMapList" resultMap="BaseDtoResultMap" parameterType="Map">
	SELECT REGORG,COUNT(0) zxNum FROM  bf_qyhznr t WHERE t.OPSTATE IN ('D','DA','X') 
	AND REGORG is not null AND update_time > DATE_SUB(NOW(),interval 5 MINUTE) GROUP BY REGORG 
    </select>
   
    
    
  
  
</mapper>