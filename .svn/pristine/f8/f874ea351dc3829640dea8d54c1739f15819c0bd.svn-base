/* 翻页 */
.notice-pagination-box {
    margin-top: 10px;
}

.notice-pagination-box .pagination2 {
    display: inline-block;
    margin-right: 0;
}

.pagination-box {
    padding: 24px 0;
}

.pagination2 {
    text-align: center;
}

.pagination2 a {
    text-decoration: none;
    border: 1px solid #AAE;
    color: #15B;
}

.pagination2 a, .pagination2 span {
    display: inline-block;
    padding: 6px 12px;
    margin: 0 2px;
    border: 1px solid #ddd;
    line-height: 1.42857143;
    color: #0066cb;
    font-size: 12px;
    background: #fff;
}

.pagination2 .current {
    background: #347ab8;
    color: #fff;
    border: 1px solid #347ab8;
    z-index: 999;
}

.pagination2 .current.prev, .pagination2 .current.next {
    color: #0066cb;
    border-color: #ddd;
    background: #fff;
    padding: 6px 12px;

}

.pagination2 a:hover {
    color: #fff;
    background-color: #347ab8;
    border: 1px solid #347ab8;
}

.pagination2 .current.prev, .pagination2 .current.next {
    cursor: not-allowed;
}

.pagination2 .icon-triangle-left,
.pagination2 .current.prev:hover .icon-triangle-left {
    display: inline-block;
    margin-right: 3px;
    width: 0;
    height: 0;
    border-top: 5px solid transparent;
    border-right: 5px solid #0066cb;
    border-bottom: 5px solid transparent;
}

.pagination2 .icon-triangle-right,
.pagination2 .current.next:hover .icon-triangle-right {
    display: inline-block;
    margin-left: 3px;
    width: 0;
    height: 0;
    border-top: 5px solid transparent;
    border-left: 5px solid #0066cb;
    border-bottom: 5px solid transparent;
}

.pagination2 a:hover .icon-triangle-left {
    border-top: 5px solid transparent;
    border-right: 5px solid #fff;
    border-bottom: 5px solid transparent;
}

.pagination2 a:hover .icon-triangle-right {
    border-top: 5px solid transparent;
    border-left: 5px solid #fff;
    border-bottom: 5px solid transparent;
}

.pagination2 .prev,
.pagination2 .prev + a,
.pagination2 .next {
    border-left: 1px solid #ddd;
    margin-left: 2px;
    margin-right: 2px;
}

