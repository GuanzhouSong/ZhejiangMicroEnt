<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<!doctype html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="renderer" content="webkit">
    <meta charset="utf-8">
    <title>业务管理_审批部门对照维护</title>
    <link rel="stylesheet" href="/css/syn.css">
</head>
<body>
    <div class="layer-bd">
        <div class="form-list">
            <div class="right"><span class="light">提示：请设置审批事项对应的具体部门</span></div>
            <div class="form-item clearfix">
                <label class="item-name col-3">审批事项选择：</label>
                <div class="col-6">
                    <div class="ipt-box col-12">
                        <input type="text" class="ipt-txt fl" placeholder="选择">
                        <span class="add-icon">
                            <i></i>
                        </span>
                    </div>
                </div>
            </div>
            <div class="form-item clearfix">
                <label class="item-name col-3">登记/属地：</label>
                <div class="col-2">
                    <div class="radio-box">
                        <label><input type="radio">登记</label>
                        <label><input type="radio">属地</label>
                    </div>
                </div>
            </div>
            <div class="form-item clearfix">
                <label class="item-name col-3">审批事项名称：</label>
                <div class="col-6">
                    <div class="ipt-box">
                        <input type="text" class="ipt-txt" placeholder="" readonly="readonly">
                    </div>
                </div>
            </div>
            <div class="form-item clearfix">
                <label class="item-name col-3">审批机关：</label>
                <div class="col-6">
                    <div class="ipt-box">
                        <input type="text" class="ipt-txt" placeholder="" readonly="readonly">
                    </div>
                </div>
            </div>
            <div class="form-item clearfix">
                <label class="item-name col-3">前置/后置：</label>
                <div class="col-2">
                    <div class="radio-box">
                        <label><input type="radio" name="loc">前置</label>
                        <label><input type="radio" name="loc">后置</label>
                    </div>
                </div>
            </div>
            <div class="form-item clearfix">
                <label class="item-name col-3">是否有效：</label>
                <div class="col-2">
                    <div class="radio-box">
                        <label><input type="radio" name="valid">有效</label>
                        <label><input type="radio" name="valid">无效</label>
                    </div>
                </div>
            </div>
            <div class="form-item clearfix">
                <label class="item-name col-3">设置人：</label>
                <div class="col-2">
                    <div class="ipt-box col-12">
                        <input type="text" class="ipt-txt">
                    </div>
                </div>
            </div>
            <div class="form-item clearfix">
                <label class="item-name col-3">设置日期：</label>
                <div class="col-2">
                    <div class="ipt-box col-12">
                        <input type="text" class="ipt-txt">
                    </div>
                </div>
            </div>
            <div class="form-item clearfix mb20">
                <label class="item-name col-3">对应审批部门：</label>
                <div class="col-6">
                    <div class="ipt-box col-12">
                        <input type="text" class="ipt-txt fl" placeholder="选择">
                        <span class="add-icon">
                            <i></i>
                        </span>
                    </div>
                </div>
            </div>
            <div class="form-item clearfix  col-offset-3">
                <div class="ml60">
                    <input type="submit" value="保 存" class="btn mr20">
                    <input type="submit" value="取 消" class="btn">
                </div>
            </div>
        </div>
    </div>
</body>
</html>