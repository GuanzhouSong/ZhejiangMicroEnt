package com.icinfo.cs.common.utils;
import java.math.BigDecimal;
/**
 * BifDecimal工具类
 * @author fanzhen
 * @date 20170510
 */
public class BigDecimalUtil {

	/**
	 * BigDecimal比较大小
	 * 大于返回true
	 * 其余返回false
	 * @return
	 */
	public static boolean BigDecimalCompareTo(BigDecimal b,int i){
		if(b.compareTo(BigDecimal.valueOf(i))==1){
			return true;
		}
		return false;
	}
}
