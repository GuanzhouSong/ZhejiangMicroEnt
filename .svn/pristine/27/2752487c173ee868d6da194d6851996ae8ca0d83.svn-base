<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<!doctype html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="renderer" content="webkit">
    <meta charset="utf-8">
    <title>信息管理数据应用反馈新增</title>
    <link rel="stylesheet" href="/css/pub-syn.css">
</head>
<body>
<div class="popup-box">
    <div class="title">添加行政处罚信息</div>
    <form id="hxForm">
    <div class="popup-hd">
        <table cellpadding="0" cellspacing="0" border="0" width="100%" class="popup-table">
            <tr>
                <td align="right">企业名称</td>
                <td>${csDepsynPunish.entName}</td>
            </tr>
            <tr>
                <td align="right">统一社会信用代码</td>
                <td>${csDepsynPunish.uniScid}</td>
            </tr>
            <tr>
                <td align="right">处罚机关</td>
                <td><input type="text" name="penAuth" value="${csDepsynPunish.penAuth}"></td>
            </tr>
            <tr>
                <td align="right">处罚决定书文号</td>
                <td><input type="text" name="penDecNo" value="${csDepsynPunish.penDecNo}"></td>
            </tr>
            <tr>
                <td align="right">处罚决定书签发日期</td>
                <td><input type="text" name="cerIssDate" value="${csDepsynPunish.cerIssDate}"></td>
            </tr>
            <tr>
                <td align="right">违法行为类型</td>
                <td><input type="text" name="illegActType" value="${csDepsynPunish.illegActType}"></td>
            </tr>
            <tr>
                <td align="right">处罚种类</td>
                <td><input type="text" name="penType" value="${csDepsynPunish.penType}"></td>
            </tr>
        </table>
    </div>
    <input type="hidden" name="id" value="${csDepsynPunish.id}">
    <div class="popup-btn-box clearfix">
        <input type="button" value="关闭" id="close" class="ipt-btn fr mb3">
        <input type="submit" value="提交" class="ipt-btn fr mr10 mb3">
    </div>
   </form>
</div>
<script src="/js/lib/require.js"></script>
<script src="/js/config.js"></script>
<script src="/js/depsyn/infomanage_edit.js"></script>
</body>
</html>