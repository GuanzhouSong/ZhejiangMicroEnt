/*css reset*/
html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, font, ins, kbd, q, s, samp, small, strike, strong, tt, var, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, caption, option {
    margin: 0;
    padding: 0;
    border: 0;
    outline: 0;
    font-weight: inherit;
    font-style: inherit;
    font-family: inherit;
    vertical-align: baseline;
}

html, body {
    height: 100%;
    box-sizing: border-box;
}

body {
    font-family: "Microsoft Yahei", Arial, "宋体";
    font-size: 12px;
}

ol, ul {
    list-style: none
}

table {
    border-collapse: collapse;
}

caption, th, td {
    font-weight: normal
}

blockquote:before, blockquote:after, q:before, q:after {
    content: ''
}

blockquote, q {
    quotes: "" ""
}

.vm {
    vertical-align: middle;
}

input {
    vertical-align: middle;
    outline: none;
    padding: 0;
}

input[readonly] {
    background-color: #fff;
}

input:disabled {
    background-color: #fff;
}

.btn:disabled {
    background-color: #cea44f;
}

input[type=checkbox], input[type=radio] {
    margin: 3px 3px 0px 5px;
    vertical-align: -2px;
}

textarea {
    resize: none;
    overflow: hidden;
}

img {
    border: none 0;
    vertical-align: middle;
    outline: none;
}

strong {
    font-weight: bold;
}

.center {
    text-align: center !important;
}

.right {
    text-align: right !important;
}

.left {
    text-align: left !important;
}

.valign-top {
    vertical-align: top;
}

.state-msg em.bdnone {
    border-right: none;
}

.pointer {
    cursor: pointer;
}

.icon-green {
    background: green;
}

.border-gray {
    border: 1px solid #ccc;
}

.dib2 {
    display: inline-block;
}

.va-3 {
    vertical-align: -3px;
}

.pt130 {
    padding-top: 130px;
}

/* 清除浮动*/
.clearfix:after {
    content: " ";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
    font-size: 0;
    line-height: 0;
}

input[type="text"]::-ms-clear {
    display: none;
}

input[type="password"]::-ms-reveal {
    display: none;
}

:-moz-placeholder, :-ms-input-placeholder {
    color: #999;
}

*.clearfix {
    zoom: 1;
}


/* 浮动*/
.fl {
    float: left !important;
}

.fr {
    float: right !important;
}

/* 相对定位*/
.pos-rel {
    position: relative;
}

/*超链接*/
a {
    text-decoration: none;
    color: #003399;
}

a:hover {
    color: #ff6600;
}

input:-webkit-autofill, textarea:-webkit-autofill, select:-webkit-autofill {
    -webkit-box-shadow: 0 0 0px 1000px white inset;
}

