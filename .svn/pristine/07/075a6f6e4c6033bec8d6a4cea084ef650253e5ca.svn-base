	<include refid="ScSpecialLibrarycount_where"/>
	<include refid="com.icinfo.cs.system.mapper.CSMapper.defaultByOrgCode"/>
	GROUP BY LEFT(IFNULL(t.RegOrg,t.localAdm),4)
