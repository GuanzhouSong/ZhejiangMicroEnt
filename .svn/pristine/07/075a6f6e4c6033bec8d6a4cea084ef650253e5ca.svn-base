  <select id="selectCheckCodeList" resultType="java.lang.String" parameterType="java.lang.String"> 
	SELECT
		n.DoublyStoSecCode
	FROM
		cs_pub_scspecial_library t
		LEFT JOIN cs_regist_mark n 
	ON t.MarkCode = n.IDCode
	WHERE t.PriPID = #{priPID}
