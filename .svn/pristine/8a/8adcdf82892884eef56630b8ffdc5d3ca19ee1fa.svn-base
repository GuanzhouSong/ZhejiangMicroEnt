	<!-- 统计入库企业营收下降数量List-->
	<select id="selectJyxjBySetDeptCodeList" parameterType="Map" resultMap="BaseResultDtoMap">
	 SELECT
	DISTINCT sb.PriPID,sb.EntName,sb.RegNO,sb.Dom,sb.LeRep,sb.RegCap,sb.EntType,sb.EstDate,sb.IndustryCo,sb.RegOrg,sb.IndustryPhy,
	sb.RegState,sb.RegCapUSD, sb.UniCode,sb.OpFrom,sb.OpTo,sb.Currency,sb.Tel,sb.Email,sb.ApprDate,sb.AltDate,sb.CurrencyCn,
	sb.OpForm,sb.OpScope,sb.EntTypeCatg, sb.RegOrgName,sb.LocalAdm,sb.LocalAdmName,sb.SliceNO,sb.SliceNOName,sb.SmentReason,
	sb.IsSment,sb.SmentRemReason, NULL EmpNum,NULL VendInc, NULL ProGro,NULL RatGro,cci.Content IndustryCoName,
	cce.Content EntTypeName, sbe.HelpState,sbe.CultivationTypeName,sbe.HelpTypeNum
	FROM
		zjcsdb1.sm_bus_entarchives sbe
	INNER JOIN zjcsdb1.cs_yr_asset cya ON sbe.PriPID = cya.PriPID
	AND cya. YEAR = (DATE_FORMAT(NOW(), '%Y') + 0 ) -1
	INNER JOIN zjcsdb1.cs_yr_asset cya2 ON cya2.PriPID = cya.PriPID
	AND cya2. YEAR = (DATE_FORMAT(NOW(), '%Y') + 0 ) -2
	INNER JOIN rpt_sm_baseinfo sb
		ON sbe.PriPID = sb.PriPID
	  LEFT JOIN zjcsdb1.sm_bus_entarchives_help sbeh
		ON sbeh.PriPID = sbe.PriPID
	  LEFT JOIN zjcsdb1.cs_code_industry cci
		ON cci.code = sb.IndustryCo
	  LEFT JOIN zjcsdb1.cs_code_enttype cce
		ON cce.code = sb.EntType
			WHERE
				sbe.setDeptCode LIKE '${deptCode}%'

			AND (
				<![CDATA[
					(cya.VendInc - cya2.VendInc) / cya2.VendInc < - 0.20
					OR (cya.ProGro - cya2.ProGro) / cya2.ProGro < - 0.20
					OR (cya.RatGro - cya2.RatGro) / cya2.RatGro < - 0.20
				]]>
			)
	</select>
	
</mapper>