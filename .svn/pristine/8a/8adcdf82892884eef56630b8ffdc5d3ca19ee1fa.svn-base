	<!-- 培育库分页列表 -->
	<select id="queryPageListForEntarchives" resultMap="BaseResultDtoMap">
	SELECT 
	A.PriPID,
	A.EntName,
	A.RegNO,
	A.Dom,
	A.LeRep,
	A.RegCap,
	A.EntType,
	A.EstDate,
	A.IndustryCo,
	A.RegOrg,
	A.IndustryPhy,
	A.RegState,
	A.RegCapUSD,
	A.UniCode,
	A.OpFrom,
	A.OpTo,
	A.Currency,
	A.Tel,
	A.Email,
	A.ApprDate,
	A.AltDate,
	A.CurrencyCn,
	A.OpForm,
	A.OpScope,
	A.EntTypeCatg,
	A.RegOrgName,
	A.LocalAdm,
	A.LocalAdmName,
	A.SliceNO,
	A.SliceNOName,
	A.SmentReason,
	A.IsSment,
	A.SmentRemReason,
	<if test="year !=null and year != ''">
		A.EmpNum,
		A.VendInc,
		A.ProGro,
		A.RatGro,
	</if>
	<if test="year ==null or year.toString() == ''">
		null EmpNum,
		null VendInc,
		null ProGro,
		null RatGro,
	</if>
	D.Content industryCoName,
    E.Content entTypeName
	FROM 
	(SELECT
	sb.PriPID,
	sb.EntName,
	sb.RegNO,
	sb.Dom,
	sb.LeRep,
	sb.RegCap,
	sb.EntType,
	sb.EstDate,
	sb.IndustryCo,
	sb.RegOrg,
	sb.IndustryPhy,
	sb.RegState,
	sb.RegCapUSD,
	sb.UniCode,
	sb.OpFrom,
	sb.OpTo,
	sb.Currency,
	sb.Tel,
	sb.Email,
	sb.ApprDate,
	sb.AltDate,
	sb.CurrencyCn,
	sb.OpForm,
	sb.OpScope,
	sb.EntTypeCatg,
	sb.RegOrgName,
	sb.LocalAdm,
	sb.LocalAdmName,
	sb.SliceNO,
	sb.SliceNOName,
	sb.SmentReason,
	sb.IsSment,
	sb.SmentRemReason,
	<if test="year !=null and year != ''">
		cya.EmpNum,
		cya.VendInc,
		cya.ProGro,
		cya.RatGro
	</if>
	<if test="year ==null or year.toString() == ''">
		null EmpNum,
		null VendInc,
		null ProGro,
		null RatGro
	</if>
	FROM
	rpt_sm_baseinfo sb
	<if test="year !=null and year != ''">
		INNER JOIN zjcsdb1.cs_yr_asset cya ON sb.PriPID = cya.PriPID and cya.YEAR = #{year}
	</if>
	LEFT JOIN zjcsdb1.sm_bus_entarchives sbe ON sb.PriPID = sbe.PriPID
	<include refid="queryPageListForEntarchivesCommonSql" />
	<include refid="com.icinfo.cs.system.mapper.CSMapper.defaultByOrgCode"/>
	order by sb.estDate desc
	LIMIT ${numStart},${pageSize}) A
	LEFT JOIN zjcsdb1.cs_code_industry D on  A.IndustryCo = D.code
	LEFT JOIN zjcsdb1.cs_code_enttype E ON A.EntType = E.code		
	</select>
	
	<sql id="queryPageListForEntarchivesCommonSql">
	    WHERE (
			sbe.PriPID is null
		or 
			EXISTS (
				SELECT
					1
				FROM
					zjcsdb1.sm_bus_entarchives_his sbeh
				WHERE
				 sbeh.PriPID = sb.PriPID
			)
		)
  		<if test="cidRegNO !=null and cidRegNO != ''">
			<![CDATA[AND (instr(sb.UniCode, #{cidRegNO}) > 0 OR instr(sb.RegNO, #{cidRegNO}) >0)]]>
		</if>
		<if test="entName !=null and entName != ''">
			<![CDATA[AND instr(sb.EntName, #{entName}) > 0]]>
		</if>
		<if test="industryCo !=null and industryCo != ''">
			<![CDATA[AND sb.IndustryCo in (${industryCo})]]>
		</if>
		<if test="entType !=null and entType != ''">
			<![CDATA[AND sb.EntType in (${entType})]]>
		</if>
		<if test="regOrg !=null and regOrg != ''">
			AND sb.RegOrg in (${regOrg})
		</if>
		<if test="estDateStart !=null and estDateStart != '' ">
  			<![CDATA[AND sb.EstDate >= #{estDateStart}	]]>
		</if>
		<if test="estDateEnd !=null and estDateEnd != '' ">
  			<![CDATA[AND sb.EstDate <= #{estDateEnd}	]]>
		</if>
		<if test="regCapStart !=null and regCapStart != '' ">
  			<![CDATA[AND sb.RegCap >= ${regCapStart}	]]>
		</if>
		<if test="regCapEnd !=null and regCapEnd != '' ">
  			<![CDATA[AND sb.RegCap <= ${regCapEnd}	]]>
		</if>
		<if test="localAdm !=null and localAdm != '' ">
			AND sb.LocalAdm in (${localAdm})
		</if>
		<if test="empNumStart !=null and empNumStart != '' ">
  			<![CDATA[AND cya.EmpNum >= ${empNumStart}	]]>
		</if>
		<if test="empNumEnd !=null and empNumEnd != '' ">
  			<![CDATA[AND cya.EmpNum <= ${empNumEnd}	]]>
		</if>
		<if test="vendIncStart !=null and vendIncStart != '' ">
  			<![CDATA[AND cya.VendInc >= ${vendIncStart}	]]>
		</if>
		<if test="vendIncEnd !=null and vendIncEnd != '' ">
  			<![CDATA[AND cya.VendInc <= ${vendIncEnd}	]]>
		</if>
	    <if test="industryType !=null and industryType != '' ">
			AND sb.industryCo in (SELECT
					cct.`Code`
				FROM
					zjcsdb1.cs_code_trade cct
				WHERE
					cct.Type = #{industryType})
		</if>
	</sql>
	
	<!-- 培育库分页列表-总数-->
	<select id="queryPageListForEntarchivesCount" resultType="Long">
	    SELECT
		count(1)
		FROM
		rpt_sm_baseinfo sb
  		<if test="year !=null and year != ''">
			INNER JOIN zjcsdb1.cs_yr_asset cya ON sb.PriPID = cya.PriPID and cya. YEAR = #{year}
		</if>
		LEFT JOIN zjcsdb1.sm_bus_entarchives sbe ON sb.PriPID = sbe.PriPID
		<include refid="queryPageListForEntarchivesCommonSql" />
		<include refid="com.icinfo.cs.system.mapper.CSMapper.defaultByOrgCode"/>
	</select>
	

	<!-- 获取企业、培育库分页列表 -->
	<select id="queryEntAndEntarchivesPageList" resultMap="BaseResultDtoMap">
		SELECT
		sb.PriPID,
		sb.EntName,
		sb.RegNO,
		sb.Dom,
		sb.LeRep,
		sb.RegCap,
		sb.EntType,
		sb.EstDate,
		sb.IndustryCo,
		sb.RegOrg,
		sb.IndustryPhy,
		sb.RegState,
		sb.RegCapUSD,
		sb.UniCode,
		sb.OpFrom,
		sb.OpTo,
		sb.Currency,
		sb.Tel,
		sb.Email,
		sb.ApprDate,
		sb.AltDate,
		sb.CurrencyCn,
		sb.OpForm,
		sb.OpScope,
		sb.EntTypeCatg,
		sb.RegOrgName,
		sb.LocalAdm,
		sb.LocalAdmName,
		sb.SliceNO,
		sb.SliceNOName,
		sb.SmentReason,
		sb.IsSment,
		sb.SmentRemReason,
		<if test="year !=null and year != ''">
			cya.EmpNum,
			cya.VendInc,
			cya.ProGro,
			cya.RatGro,
		</if>
		<if test="year ==null or year.toString() == ''">
			null EmpNum,
			null VendInc,
			null ProGro,
			null RatGro,
		</if>
		cci.Content IndustryCoName,
		cce.Content EntTypeName,
		sbe.HelpState,
		sbe.CultivationTypeName,
		sbe.HelpTypeNum
		FROM
		rpt_sm_baseinfo sb
		INNER JOIN zjcsdb1.sm_bus_entarchives sbe ON sbe.PriPID = sb.PriPID
		LEFT JOIN
		zjcsdb1.sm_bus_entarchives_help sbeh ON sbeh.PriPID = sbe.PriPID
		<if test="year !=null and year != ''">
  		LEFT JOIN zjcsdb1.cs_yr_asset cya ON sb.PriPID = cya.PriPID and cya. YEAR = #{year}
        </if>
		LEFT JOIN zjcsdb1.cs_code_industry
		cci ON cci.code = sb.IndustryCo
		LEFT JOIN zjcsdb1.cs_code_enttype cce ON cce.code = sb.EntType
		WHERE
		NOT EXISTS (
			SELECT
			1
			FROM
			zjcsdb1.sm_bus_entarchives_his sbeh
			WHERE
			sbeh.PriPID = sbe.PriPID
		)
		<if test="cultivationTypeCode !=null and cultivationTypeCode != ''">
			AND sbe.CultivationTypeCode = #{cultivationTypeCode}
		</if>
		<if test="areaTypeCode !=null and areaTypeCode != ''">
			AND (sbe.Tsxz REGEXP ('${areaTypeCode}') or Zckj REGEXP
			('${areaTypeCode}') or Kjfhy REGEXP ('${areaTypeCode}') or Kjdsy REGEXP
			('${areaTypeCode}') or Xwqyy REGEXP('${areaTypeCode}') or Qt REGEXP ('${areaTypeCode}'))
		</if>
		<if test="industryType !=null and industryType != '' and industryType != 0">
			AND FIND_IN_SET(#{industryType},IndustryType) 
		</if>
		<if test="industryType !=null and industryType != '' and industryType == 0">
			AND IndustryType is not null
		</if>
		<if test="entCrowdCode !=null and entCrowdCode != ''">
			AND EntCrowdCode = #{entCrowdCode}
		</if>
		<if test="helpTypeCode !=null and helpTypeCode != ''">
			AND HelpTypeCode = #{helpTypeCode}
		</if>
		<if test="helpState !=null and helpState != ''">
			AND HelpState = #{helpState}
		</if>
		<if test="helpStateWelcome !=null and helpStateWelcome != '' and helpStateWelcome == 1">
			AND HelpState = 1
		</if>
		<if test="helpStateWelcome !=null and helpStateWelcome != '' and helpStateWelcome == 2">
			AND (HelpState = 2 OR HelpState = 3)
		</if>

		<if test="cidRegNO !=null and cidRegNO != ''">
			<![CDATA[AND (instr(sb.UniCode, #{cidRegNO}) > 0 OR instr(sb.RegNO, #{cidRegNO}) >0)]]>
		</if>
		<if test="entName !=null and entName != ''">
			<![CDATA[AND instr(sb.EntName, #{entName}) > 0]]>
		</if>
		<if test="industryCo !=null and industryCo != ''">
			<![CDATA[AND sb.IndustryCo in (${industryCo})]]>
		</if>
		<if test="entType !=null and entType != ''">
			<![CDATA[AND sb.EntType in (${entType})]]>
		</if>
		<if test="regOrg !=null and regOrg != ''">
			AND sb.RegOrg in (${regOrg})
		</if>
		<if test="estDateStart !=null and estDateStart != '' ">
  			<![CDATA[AND sb.EstDate >= #{estDateStart}	]]>
		</if>
		<if test="estDateEnd !=null and estDateEnd != '' ">
  			<![CDATA[AND sb.EstDate <= #{estDateEnd}	]]>
		</if>
		<if test="regCapStart !=null and regCapStart != '' ">
  			<![CDATA[AND sb.RegCap >= ${regCapStart}	]]>
		</if>
		<if test="regCapEnd !=null and regCapEnd != '' ">
  			<![CDATA[AND sb.RegCap <= ${regCapEnd}	]]>
		</if>
		<if test="localAdm !=null and localAdm != '' ">
			AND sb.LocalAdm in (${localAdm})
		</if>
		<if test="empNumStart !=null and empNumStart != '' ">
  			<![CDATA[AND cya.EmpNum >= ${empNumStart}	]]>
		</if>
		<if test="empNumEnd !=null and empNumEnd != '' ">
  			<![CDATA[AND cya.EmpNum <= ${empNumEnd}	]]>
		</if>
		<if test="vendIncStart !=null and vendIncStart != '' ">
  			<![CDATA[AND cya.VendInc >= ${vendIncStart}	]]>
		</if>
		<if test="vendIncEnd !=null and vendIncEnd != '' ">
  			<![CDATA[AND cya.VendInc <= ${vendIncEnd}	]]>
		</if>
		<include refid="com.icinfo.cs.system.mapper.CSMapper.defaultByOrgCode"/>
		GROUP BY sb.id 
		order by sbe.UpdateTime desc
	</select>

	<!-- 根据企业id获取对象 -->
	<select id="queryBeanByPriPID" resultMap="BaseResultDtoMap">
		select sb.EntName,cci.Content IndustryCoName,sb.EstDate,sb.Dom,sb.PostalCode
		from rpt_sm_baseinfo sb
		LEFT JOIN zjcsdb1.cs_code_industry cci ON cci.code = sb.IndustryCo
