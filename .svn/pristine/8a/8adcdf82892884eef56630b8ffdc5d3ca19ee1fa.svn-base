	<select id="countByHelp" parameterType="Map" resultType="Integer">
		SELECT
		  COUNT(1)
		FROM
		  (SELECT
			sb.PriPID
		  FROM
			rpt_sm_baseinfo sb
			INNER JOIN zjcsdb1.sm_bus_entarchives sbe
			  ON sbe.PriPID = sb.PriPID
			LEFT JOIN zjcsdb1.sm_bus_entarchives_help sbeh
			  ON sbeh.PriPID = sbe.PriPID
			LEFT JOIN zjcsdb1.cs_code_industry cci
			  ON cci.code = sb.IndustryCo
			LEFT JOIN zjcsdb1.cs_code_enttype cce
			  ON cce.code = sb.EntType
		  WHERE NOT EXISTS
			(SELECT
			  1
			FROM
			  zjcsdb1.sm_bus_entarchives_his sbeh
			WHERE sbeh.PriPID = sbe.PriPID)
		<if test="helpState !=null and helpState != '' and helpState==1">
			AND sbe.HelpState = 1
		</if>
		<if test="helpState !=null and helpState != '' and helpState==2">
			AND (sbe.HelpState =2 OR sbe.HelpState =3)
		</if>
		<include refid="com.icinfo.cs.system.mapper.CSMapper.defaultByOrgCode"/>
		  GROUP BY sb.id) table_count

	</select>
	
