		  cce.Content EntTypeName, sbe.HelpState,sbe.CultivationTypeName,sbe.HelpTypeNum
		FROM
		zjcsdb1.sm_bus_entarchives sbe
		INNER JOIN rpt_sm_baseinfo sb ON sb.PriPID = sbe.PriPID
		AND sb.IsSment = '0'
		INNER JOIN zjcsdb1.cs_yr_asset cya ON sb.PriPID = cya.PriPID
