	<!-- 统计入库企业上规升级数量-->
	<select id="countSgsjBySetDeptCode" resultType="Long" parameterType="String">
     SELECT
		count(DISTINCT sbe.PriPID )
	FROM
		zjcsdb1.sm_bus_entarchives sbe
	INNER JOIN rpt_sm_baseinfo sb ON sb.PriPID = sbe.PriPID
	AND sb.IsSment = '0'
	INNER JOIN zjcsdb1.cs_yr_asset cya ON sb.PriPID = cya.PriPID
	AND cya. YEAR = (DATE_FORMAT(NOW(), '%Y') + 0 ) -1 
	INNER JOIN zjcsdb1.cs_yr_asset cya2 ON cya2.PriPID = cya.PriPID
	AND cya2. YEAR = (DATE_FORMAT(NOW(), '%Y') + 0 ) -2 
	WHERE
		<![CDATA[cya.VendInc > cya2.VendInc]]>
	AND	sbe.setDeptCode like '${_parameter}%'
	</select>

