	</select>

	<select id="selectListByCondition" parameterType="map" resultMap="BaseResultDtoMap">
		SELECT
		A.id,
		A.PriPID,
		A.EntName,
		A.RegNO,
		A.Dom,
		A.LeRep,
		A.RegCap,
		A.EntType,
		A.EstDate,
		A.IndustryCo,
		A.RegOrg,
		A.RegOrgName,
		A.IndustryCo,
		A.IndustryPhy,
		A.RegState,
		A.RegCapUSD,
		A.UniCode,
		A.OpFrom,
		A.OpTo,
		A.Currency,
		A.Tel,
		A.Email,
		A.ApprDate,
		A.AltDate,
		A.UID,
		A.CurrencyCn,
		A.PostalCode,
		A.OpForm,
		A.OpScope,
		A.EntTypeCatg,
		A.LocalAdm,
		A.LocalAdmName,
