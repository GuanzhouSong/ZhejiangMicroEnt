/*动画帧*/
@keyframes turn2 {
    0% {
        transform: rotateX(0)
    }

    100% {
        transform: rotateX(-90deg)
    }
}

@-moz-keyframes turn2 {
    0% {
        -moz-transform: rotateX(0)
    }

    100% {
        -moz-transform: rotateX(-90deg)
    }
}

@-webkit-keyframes turn2 {
    0% {
        -webkit-transform: rotateX(0)
    }

    100% {
        -webkit-transform: rotateX(-90deg)
    }
}

@keyframes show {
    0% {
        opacity: 0
    }

    100% {
        opacity: 1
    }
}

@-moz-keyframes show {
    0% {
        opacity: 0
    }

    100% {
        opacity: 1
    }
}

@-webkit-keyframes show {
    0% {
        opacity: 0
    }

    100% {
        opacity: 1
    }
}

@keyframes showh {
    0% {
        opacity: 1
    }

    97% {
        opacity: 1
    }

    100% {
        opacity: 0
    }

}

@-moz-keyframes showh {
    0% {
        opacity: 1
    }

    97% {
        opacity: 1
    }

    100% {
        opacity: 0
    }

}

@-webkit-keyframes showh {
    0% {
        opacity: 1
    }

    97% {
        opacity: 1
    }

    100% {
        opacity: 0
    }

}

@keyframes hide {
    0% {
        opacity: 1
    }

    100% {
        opacity: 0
    }
}

@-moz-keyframes hide {
    0% {
        opacity: 1
    }

    100% {
        opacity: 0
    }
}

@-webkit-keyframes hide {
    0% {
        opacity: 1
    }

    100% {
        opacity: 0
    }
}


.mvc-top-num{
    color: #FFF;
    font: normal 58px/68px 'Microsoft YaHei';
}

.mvc-top-num ul{
    float: left;
    margin-left: 2px;
    border: 1px solid #0d568f;
    border-radius: 3px;
    width: 36px;
    box-sizing: border-box;
    perspective: 200px;
    -webkit-perspective:200px;
    position: relative;
    height: 70px;
}

.mvc-top-num ul:first-child{
    margin-left: 0;
}

.mvc-top-num li{
    position: absolute;
    top:0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 0;
}

.mvc-top-num li:first-child{
    z-index: 1;
}

.mvc-top-num li .up{
    position: absolute;
    top:0;
    left:0;
    width: 100%;
    height: 50%;
    transform-origin: 50% 100%;
    /*background-color: #1f1746;*/
    overflow: hidden;
}

.mvc-top-num li .down{
    position: absolute;
    top:50%;
    left:0;
    width: 100%;
    height: 50%;
    transform-origin: 50% 0;
    overflow: hidden;
    /*background-color: #1f1746;*/
}

.mvc-top-num .shadow{
    position: absolute;
    top:0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 2;
}

.mvc-top-num li .inn{
    position: absolute;
    left: 0;
    top:0;
    z-index: 0;
    width: 100%;
    height: 200%;
    color: #fff;
    text-align: center;
    background: #1e2c66;
    border-radius: 4px;
}

.mvc-top-num li.before {
    z-index: 3;
}

.mvc-top-num .down .inn{
    top: -34px;
}

.mvc-top-num .before .down .inn{
    animation: hide .2s .3s linear both;
    -webkit-animation: hide .2s .3s linear both;
}

.mvc-top-num li.before .up {
    z-index: 2;
    animation: turn2 .5s linear both;
    -moz-animation: turn2 .5s linear both;
    -ms-animation: turn2 .5s linear both;
    -o-animation: turn2 .5s linear both;
    -webkit-animation: turn2 .5s linear both
}

.mvc-top-num li.before .up .shadow {
    background: #1e2c66;
    border-radius: 6px;
    animation: show .5s linear both;
    -moz-animation: show .5s linear both;
    -ms-animation: show .5s linear both;
    -o-animation: show .5s linear both;
    -webkit-animation: show .5s linear both
}

.mvc-top-num li.before .down .shadow {
    background: #1e2c66;
    border-radius: 6px;
    animation: showh .5s linear both;
    -moz-animation: showh .5s linear both;
    -ms-animation: showh .5s linear both;
    -o-animation: showh .5s linear both;
    -webkit-animation: showh .5s linear both
}

