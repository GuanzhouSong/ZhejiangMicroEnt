/*css reset*/
html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, font, ins, kbd, q, s, samp, small, strike, strong, tt, var, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, caption, option {
    margin: 0;
    padding: 0;
    border: 0;
    outline: 0;
    font-weight: inherit;
    font-style: inherit;
    font-family: inherit;
    vertical-align: baseline;
}

textarea::-webkit-input-placeholder:after{
    display:block;
    content:"line@ \A line#";/*  \A 表示换行  */
    color:red;
}

html, body {
    height: 100%;
    box-sizing: border-box;

}

body {
    font-family: "Microsoft Yahei", Arial, "宋体";
    font-size: 12px;
}

ol, ul {
    list-style: none
}

table {
    border-collapse: collapse;
}

caption, th, td {
    font-weight: normal
}

blockquote:before, blockquote:after, q:before, q:after {
    content: ''
}

blockquote, q {
    quotes: "" ""
}

.vm {
    vertical-align: middle;
}

input {
    vertical-align: middle;
    outline: none;
    padding: 0;
}

input[readonly] {
    background-color: #fff;
}

input:disabled {
    background-color: #fff;
}

.btn:disabled {
    background-color: #437ecc;
}

input[type=checkbox], input[type=radio] {
    margin: 3px 3px 0px 5px;
    vertical-align: -2px;
}

textarea {
    resize: none;
    overflow: hidden;
    overflow-y: auto !important;
}

img {
    border: none 0;
    vertical-align: middle;
    outline: none;
}

strong {
    font-weight: bold;
}

.center {
    text-align: center !important;
}

.right {
