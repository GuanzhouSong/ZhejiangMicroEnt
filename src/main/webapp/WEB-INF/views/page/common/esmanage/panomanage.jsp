<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<!doctype html>
<html lang="en">
<head>
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta name="renderer" content="webkit">
<meta charset="utf-8">
<title>全景查询索引维护页面</title>
<link rel="stylesheet" href="/css/syn.css">
<link rel="shortcut icon" href="/img/favicon.ico" type="image/x-icon" />
</head>
<body class="pd10">
	<div>
		<div class="print-nocard mb20">
			<h3 style="color: #bb913e;">全景查询索引维护</h3>
		</div>
		<hr />
		<br />
		<form id="hx-form">
			<div class="form-list">
				<div class="form-item clearfix">
					<label class="item-name col-2" style="color:red; font-size: 16px;font-weight: bold; ">索引更新</label>
					<label class="item-name col-1"> 更新主体</label>
					<div class="col-3">
						<div class="ipt-box">
							<input type="text" class="ipt-txt" name="keyword"
								placeholder="企业名称/注册号/统一社会代码/内部序号">
						</div>
					</div>
					<label class="item-name col-1"> 更新密码</label>
					<div class="col-3">
						<div class="ipt-box">
							<input type="text" class="ipt-txt" name="upassword">
						</div>
					</div>
					<div class="col-2">
						<input type="submit" value="提 交" class="btn mr20"> 
					</div>
				</div>


			</div>
		</form>
		
		<hr />
		<br />
		<form id="hx-form-delete">
			<div class="form-list">
				<div class="form-item clearfix">
					<label class="item-name col-2" style="color:red; font-size: 16px;font-weight: bold; ">索引删除</label>
					<label class="item-name col-1"> 内部序号</label>
					<div class="col-3">
						<div class="ipt-box">
							<input type="text" class="ipt-txt" name="keyword_delete" placeholder="内部序号">
						</div>
					</div>
					<label class="item-name col-1"> 删除密码</label>
					<div class="col-3">
						<div class="ipt-box">
							<input type="text" class="ipt-txt" name="upassword_delete">
						</div>
					</div>
					<div class="col-2">
						<input type="submit" value="提 交" class="btn mr20">
					</div>
				</div>
			</div>
		</form>

	</div>



	<script src="/js/lib/require.js"></script>
	<script src="/js/config.js"></script>
	<script src="/js/lib/laydate/laydate.js"></script>
	<script src="/js/common/idxmanage/panomanage.js"></script>
</body>
</html>