<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<!doctype html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="renderer" content="webkit">
    <meta charset="utf-8">
    <title>业务管理_部门名称规范维护</title>
    <link rel="stylesheet" href="/css/syn.css">
</head>
<body>
<div class="layder-bd">
    <div class="form-list name-specification">
        <div class="form-item clearfix">
            <label class="item-name col-3">行政编码：</label>
            <div class="col-4">
                <div class="ipt-box mr10">
                    <input type="text" class="ipt-txt">
                </div>
            </div>
            <div class="example col-5">
                示例：330106
            </div>
        </div>
        <div class="form-item clearfix">
            <label class="item-name col-3"><span class="light">*</span>部门全称：</label>
            <div class="col-4">
                <div class="ipt-box mr10">
                    <input type="text" class="ipt-txt">
                </div>
            </div>
            <div class="example col-5">
                示例：杭州市市场监督管理局西湖分局
            </div>
        </div>
        <div class="form-item clearfix">
            <label class="item-name col-3"><span class="light">*</span>部门简称：</label>
            <div class="col-4">
                <div class="ipt-box mr10">
                    <input type="text" class="ipt-txt">
                </div>
            </div>
            <div class="example col-5">
                示例：西湖市监分局
            </div>
        </div>
        <div class="form-item clearfix">
            <label class="item-name col-3"><span class="light">*</span>部门字号：</label>
            <div class="col-4">
                <div class="ipt-box mr10">
                    <input type="text" class="ipt-txt">
                </div>
            </div>
            <div class="example col-5">
                示例：西市监
            </div>
        </div>
        <div class="form-item clearfix">
            <label class="item-name col-3"><span class="light">*</span>同级人员政府：</label>
            <div class="col-4">
                <div class="ipt-box col-8 mr2">
                    <input type="text" class="ipt-txt">
                </div>
                <div class="item-txt">人员政府</div>
            </div>
            <div class="example col-5">
                示例：西湖区  人民政府
            </div>
        </div>
        <div class="form-item clearfix mb20">
            <label class="item-name col-3"><span class="light">*</span>同级人员法院：</label>
            <div class="col-4">
                <div class="ipt-box col-8 mr2">
                    <input type="text" class="ipt-txt">
                </div>
                <div class="item-txt">人员法院</div>
            </div>
            <div class="example h32 col-5">
                示例：西湖区  人民法院
            </div>
        </div>
        <div class="form-item clearfix col-offset-3">
            <div class="ml60">
                <input type="submit" value="提 交" class="btn mr20">
                <input type="submit" value="取 消" class="btn">
            </div>
        </div>
    </div>
</div>
</body>
</html>