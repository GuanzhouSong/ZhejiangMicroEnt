<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<!doctype html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="renderer" content="webkit">
    <meta charset="utf-8">
    <title>社保信息</title>
    <link rel="stylesheet" type="text/css" href="/css/reg.client.css">
</head>
<body>
<div class="mod">
    <div class="mod-hd">
        <div class="title">
            <h3>企业社保信息</h3>
            <div class="currency">币种：<strong class="light">人民币</strong></div>
        </div>
    </div>
    <div class="mod-bd">
        <div class="content mod-bd-panel">
            <div class="con-title">参保险种及对应人数</div>
            <div class="w-960">
                <table class="table-public table-caiwu" border="0" cellspacing="0" cellpadding="0">
                    <tbody>
                    <tr>
                        <td class="odd focus-state">
                            <div class="item-name w-160">城镇职工基本养老保险</div>
                            <div class="ipt-box js-ipt-box">
                                <input type="text" id="" name="" value="" class="ipt-txt" placeholder="">
                                <i class="icon-close"></i>
                            </div>
                            人
                        </td>
                        <td class="even focus-state">
                            <div class="item-name w-160">失业保险</div>
                            <div class="ipt-box js-ipt-box">
                                <input type="text" id="" name="" value="" class="ipt-txt" placeholder="">
                                <i class="icon-close"></i>
                            </div>
                            人
                        </td>
                    </tr>
                    <tr>
                        <td class="odd focus-state">
                            <div class="item-name w-160">职工基本医疗保险</div>
                            <div class="ipt-box js-ipt-box">
                                <input type="text" id="" name="" value="" class="ipt-txt" placeholder="">
                                <i class="icon-close"></i>
                            </div>
                            人
                        </td>
                        <td class="even focus-state">
                            <div class="item-name w-160">工伤保险</div>
                            <div class="ipt-box js-ipt-box">
                                <input type="text" id="" name="" value="" class="ipt-txt" placeholder="">
                                <i class="icon-close"></i>
                            </div>
                            人
                        </td>
                    </tr>
                    <tr>
                        <td class="odd focus-state">
                            <div class="item-name w-160">生育保险</div>
                            <div class="ipt-box js-ipt-box">
                                <input type="text" id="" name="" value="" class="ipt-txt" placeholder="">
                                <i class="icon-close"></i>
                            </div>
                            人
                        </td>
                    </tr>
                    </tbody>
                </table>
            </div>
            <div class="con-title">参保基数及金额</div>
            <div class="w-960">
                <table class="table-public table-caiwu" border="0" cellspacing="0" cellpadding="0">
                    <tbody>
                    <tr>
                        <td class="odd focus-state">
                            <div class="item-name w-160">单位缴费基数</div>
                            <div class="ipt-box js-ipt-box">
                                <input type="text" id="" name="" value="" class="ipt-txt" placeholder="0.00">
                                <i class="icon-close"></i>
                            </div>
                            <span class="light">万元</span>
                            <div class="radio-box">
                                <label><input type="radio" name="liaGroIsPub" value="1">公示</label>
                                <label><input type="radio" name="liaGroIsPub" value="0" checked="">不公示</label>
                            </div>
                        </td>
                        <td class="even focus-state">
                            <div class="item-name w-160">单位累计欠缴金额</div>
                            <div class="ipt-box js-ipt-box">
                                <input type="text" id="" name="" value="" class="ipt-txt" placeholder="0.00">
                                <i class="icon-close"></i>
                            </div>
                            <span class="light">万元</span>
                            <div class="radio-box">
                                <label><input type="radio" name="li" value="1">公示</label>
                                <label><input type="radio" name="li" value="0" checked="">不公示</label>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td class="odd focus-state">
                            <div class="item-name w-160">本期实际缴费金额</div>
                            <div class="ipt-box js-ipt-box">
                                <input type="text" id="" name="" value="" class="ipt-txt" placeholder="">
                                <i class="icon-close"></i>
                            </div>
                            <span class="light">万元</span>
                            <div class="radio-box">
                                <label><input type="radio" name="l" value="1">公示</label>
                                <label><input type="radio" name="l" value="0" checked="">不公示</label>
                            </div>
                        </td>
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <div class="btn-box">
            <button id="" type="submit" class="btn-common btn-save">保存</button>
            <button id="" type="submit" class="btn-common btn-save">取消</button>
        </div>
    </div>
</div>
</body>
</html>