<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<!doctype html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="renderer" content="webkit">
    <meta charset="utf-8">
    <title>地址维护-新增</title>
    <link rel="stylesheet" href="/css/syn.css">
</head>
<body>
<div class="form-list w570">
    <div class="form-item clearfix clearfix">
        <label class="item-name col-4"><span class="light">*</span>送达单位名称：</label>
        <div class="col-6">
            <div class="ipt-box">
                <input type="text" class="ipt-txt">
            </div>
        </div>
    </div>
    <div class="form-item clearfix clearfix">
        <label class="item-name col-4"><span class="light">*</span>送达地址：</label>
        <div class="col-6">
            <div class="ipt-box">
                <input type="text" class="ipt-txt">
            </div>
        </div>
    </div>
    <div class="form-item clearfix">
        <label class="item-name col-4">联系人：</label>
        <div class="col-6">
            <div class="ipt-box">
                <input type="text" class="ipt-txt">
            </div>
        </div>
    </div>
    <div class="form-item clearfix">
        <label class="item-name col-4"> <span class="light">*</span>联系电话：</label>
        <div class="col-6">
            <div class="ipt-box">
                <input type="text" class="ipt-txt">
            </div>
        </div>
    </div>
    <div class="form-item clearfix">
        <label class="item-name col-4"> <span class="light">*</span>邮政编码：</label>
        <div class="col-3">
            <div class="ipt-box">
                <input type="text" class="ipt-txt">
            </div>
        </div>
    </div>
    <div class="form-item clearfix">
        <label class="item-name col-4"><span class="light">*</span>是否有效：</label>
        <div class="col-6">
            <div class="radio-box">
                <label><input type="radio">是</label>
                <label><input type="radio">否</label>
            </div>
        </div>
    </div>
    <div class="form-item clearfix">
        <label class="item-name col-4"> 备注：</label>
        <div class="col-6">
            <div class="ipt-box">
                <input type="text" class="ipt-txt">
            </div>
        </div>
    </div>
    <div class="form-item clearfix">
        <label class="item-name col-4"> 设置人：</label>
        <div class="col-6">
            <div class="item-txt">d</div>
        </div>
    </div>
    <div class="form-item clearfix">
        <label class="item-name col-4"> 设置日期：</label>
        <div class="col-6">
            <div class="item-txt">d</div>
        </div>
    </div>
</div>
</body>
</html>